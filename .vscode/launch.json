    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/loremipsum.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Optimized Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/optimized-test-code/forstatement.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run Tests",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
            "args": ["--config","jest.config.js"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run Web Tests",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
            "args": ["--config","jest.web.config.js"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run Test Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/test-web/server.js"
        }
    ]
}